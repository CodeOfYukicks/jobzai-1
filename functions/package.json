{
  "name": "functions",
  "version": "1.0.0",
  "description": "Firebase Functions for JobzAI",
  "main": "lib/index.js",
  "scripts": {
    "lint": "echo 'No linting configured'",
    "build": "tsc -p tsconfig.json",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "debug:greenhouse": "ts-node scripts/testFetchGreenhouse.ts",
    "debug:lever": "ts-node scripts/testFetchLever.ts",
    "debug:ats": "ts-node scripts/testAllSources.ts",
    "check:prod": "ts-node scripts/checkProdData.ts",
    "seed:emulator": "ts-node scripts/seedEmulator.ts",
    "fetch:local": "ts-node scripts/fetchJobsLocal.ts"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.4",
    "@types/mailgun-js": "^0.22.18",
    "@types/turndown": "^5.0.6",
    "body-parser": "^1.20.3",
    "cheerio": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "firebase-admin": "^11.0.0",
    "firebase-functions": "^4.0.0",
    "mailgun-js": "^0.22.0",
    "node-fetch": "2.6.7",
    "openai": "^4.85.4",
    "stripe": "^19.3.0",
    "turndown": "^7.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.0"
  },
  "private": true
}
